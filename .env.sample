
# 애플리케이션 설정
APP_NAME=*
APP_VERSION=1.0.0
ENVIRONMENT=development  # development, production, testing
DEBUG=True

# API 설정
API_PREFIX=*

# CORS 설정 (콤마로 구분된 도메인 목록 또는 * 사용)
CORS_ORIGINS=*

# 데이터 파일 경로 (절대 경로 또는 상대 경로)
DATA_FILE_PATH=*

# 검색 설정
DEFAULT_LIMIT=10
MAX_LIMIT=100

# 로깅 설정
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL

# 데이터베이스 연결 문자열
DATABASE_URL=postgresql://trademark_user:your_password@localhost:5432/db_name